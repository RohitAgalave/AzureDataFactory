			"referenceName": "AzureBlobStorageLS",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"TableName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat('dbo_',dataset().TableName,'_',utcnow(),'.csv')",
